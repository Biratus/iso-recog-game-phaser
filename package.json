{
  "name": "phaser3-typescript-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Phaser3 Typescript Webpack Boilerplate",
  "scripts": {
    "build:web": "python assets/generateAssets.py && cross-env PLATFORM=web webpack --config webpack.prod.js",
    "build:android": "python assets/generateAssets.py && cross-env PLATFORM=android webpack --config webpack.prod.js",
    "build:ios": "python assets/generateAssets.py && cross-env PLATFORM=ios webpack --config webpack.prod.js",
    "dev": "cross-env PLATFORM=web webpack-dev-server --config webpack.dev.js"
  },
  "author": "Vlad Faust <mail@vladfaust.com>",
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^5.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.1",
    "dotenv": "^5.0.0",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "outlines": "git+https://github.com/Biratus/outlines.git",
    "phaser": "^3.19.0",
    "phaser3-plugin-isometric": "git+https://github.com/Biratus/phaser3-plugin-isometric.git",
    "raw-loader": "^0.5.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "underscore": "^1.9.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
